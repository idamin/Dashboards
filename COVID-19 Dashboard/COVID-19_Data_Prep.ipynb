    "covid_df['Confirmed_Lag'] = covid_df.Confirmed.shift(1).fillna(0)\n",
    "covid_df['Deaths_Lag'] = covid_df.Deaths.shift(1).fillna(0)\n",
    "covid_df['Recoveries_Lag'] = covid_df.Recoveries.shift(1).fillna(0)\n",
    "\n",
    "covid_df['Daily Confirmed'] = covid_df.Confirmed - covid_df.Confirmed_Lag\n",
    "covid_df['Daily Deaths'] = covid_df.Deaths - covid_df.Deaths_Lag\n",
    "covid_df['Daily Recoveries'] = covid_df.Recoveries - covid_df.Recoveries_Lag\n",
    "\n",
    "covid_df.drop('Confirmed_Lag', inplace=True, axis = 1)\n",
    "covid_df.drop('Deaths_Lag', inplace=True, axis = 1)\n",
    "covid_df.drop('Recoveries_Lag', inplace=True, axis = 1)\n",
    "\n",
    "covid_df\n"
   ]
